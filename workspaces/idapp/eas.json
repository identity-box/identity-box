{
  "cli": {
    "version": ">= 0.56.0"
  },
  "build": {
    "production": {
      "node": "20.13.1",
      "cache": {
        "key": "2"
      }
    },
    "development": {
      "extends": "production",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "extends": "production",
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      }
    }
  },
  "submit": {
    "production": {}
  }
}