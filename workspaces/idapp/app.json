{
  "expo": {
    "name": "Identity App",
    "slug": "idapp",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "scheme": "idapp",
    "web": {
      "bundler": "metro"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "online.idbox.idapp",
      "versionCode": 33
    },
    "ios": {
      "buildNumber": "33",
      "bundleIdentifier": "online.idbox.idapp",
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCameraUsageDescription": "Identity App uses the camera to scan QR codes."
      }
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "extra": {
      "eas": {
        "projectId": "245dd760-cb76-11e9-9a82-0d0aebe5d6fa"
      }
    }
  }
}